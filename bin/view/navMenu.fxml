<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<BorderPane stylesheets="application/application.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.NavMenuController">
   <left>
      <AnchorPane fx:id="anchorMenu" prefHeight="139.0" prefWidth="1000.0" styleClass="navMenu" BorderPane.alignment="CENTER">
         <children>
            <StackPane layoutX="56.0" layoutY="12.0" onMouseClicked="#goToHome">
               <children>
                  <ImageView fx:id="buttonHome" fitHeight="115.0" fitWidth="157.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../assets/img/post_it.png" />
                     </image>
                  </ImageView>
                  <Label rotate="-12.1" text="Accueil" textAlignment="CENTER">
                     <font>
                        <Font name="Bernard MT Condensed" size="22.0" />
                     </font>
                  </Label>
               </children>
            </StackPane>

            <StackPane layoutX="295.0" layoutY="12.0" onMouseClicked="#goTolstReserv">
               <children>
                  <ImageView fx:id="buttonLstReserv" fitHeight="115.0" fitWidth="157.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../assets/img/post_it.png" />
                     </image>
                  </ImageView>
                  <Label prefHeight="56.0" prefWidth="104.0" rotate="-12.1" text="Liste des réservations" textAlignment="CENTER" wrapText="true">
                     <font>
                        <Font name="Bernard MT Condensed" size="22.0" />
                     </font>
                  </Label>
               </children>
            </StackPane>

            <StackPane layoutX="536.0" layoutY="12.0" onMouseClicked="#goToCreateReserv">
               <children>
                  <ImageView fx:id="buttonCreateReserv" fitHeight="115.0" fitWidth="157.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../assets/img/post_it.png" />
                     </image>
                  </ImageView>
                  <Label prefHeight="76.0" prefWidth="105.0" rotate="-12.0" text="Créer une réservation" textAlignment="CENTER" wrapText="true">
                     <font>
                        <Font name="Bernard MT Condensed" size="22.0" />
                     </font>
                  </Label>
               </children>
            </StackPane>

            <StackPane layoutX="783.0" layoutY="12.0" onMouseClicked="#goToRoom">
               <children>
                  <ImageView fx:id="buttonRoom" fitHeight="115.0" fitWidth="157.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../assets/img/post_it.png" />
                     </image>
                  </ImageView>
                  <Label rotate="-12.0" text="Salle">
                     <font>
                        <Font name="Bernard MT Condensed" size="22.0" />
                     </font>
                  </Label>
               </children>
            </StackPane>
         </children>
      </AnchorPane>
   </left>
</BorderPane>
